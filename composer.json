{
  "name": "rolandschale/consultation-hours-bundle",
  "description": "Erfassung der Sprechzeiten einer Praxis",
  "keywords": [
    "contao",
    "bundle",
    "tag 3",
    "tag 4"
  ],
  "type": "contao-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Roland Schale",
      "email": "roland.schale@gmail.com",
      "homepage": "https://github.com/rolandschale/consultation-hours",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/rolandschale/consultation-hours-bundle/issues",
    "source": "https://github.com/rolandschale/consultation-hours-bundle"
  },
  "require": {
    "contao/core-bundle": "^4.9",
    "rolandschale/consultation-hours": "dev-main"
  },
  "require-dev": {
    "contao/easy-coding-standard": "^3.0",
    "contao/manager-plugin": "^2.3.1",
    "contao/test-case": "^4.6",
    "phpunit/phpunit": "^9.3",
    "symfony/phpunit-bridge": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Rolandschale\\ConsultationHoursBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/templates"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Rolandschale\\ConsultationHoursBundle\\ContaoManager\\Plugin"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/rolandschale/consultation-hours-bundle"
    }
  ],
  "version": "1.0"
}
